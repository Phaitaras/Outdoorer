{
  "expo": {
    "name": "Outdoorer",
    "slug": "outdoorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app.icon",
    "scheme": "outdoorer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.phaitaras.outdoorer",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs your location to provide location-based services while you are using the app.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs your location to track your movements even in the background.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.phaitaras.outdoorer"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Outdoorer to use your location."
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFAE00",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/splash-icon.png",
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.88192225830-8oer9nc590c2fmdb08upnjr49lqd43md"
        }
      ],
      "expo-sqlite",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "80b2857c-7fc1-480e-b3e7-3a82ac945595"
      }
    },
    "owner": "phaitaras"
  }
}